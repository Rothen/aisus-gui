{
    "name": "gui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "download-openapi": "curl -O http://localhost:5000/aisus-openapi.json",
        "gs": "openapi-generator-cli generate -i aisus-openapi.json -g typescript-angular -o src/app/modules/openapi --additional-properties fileNaming=kebab-case,withInterfaces=true --generate-alias-as-model --openapi-generator-ignore-list \"README.md,.gitignore,.openapi-generator-ignore,git_push.sh,index.ts,.openapi-generator\"",
        "gswc": "openapi-generator-cli generate -g typescript-angular -c openapi-config.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.2.0",
        "@angular/common": "^18.2.0",
        "@angular/compiler": "^18.2.0",
        "@angular/core": "^18.2.0",
        "@angular/forms": "^18.2.0",
        "@angular/platform-browser": "^18.2.0",
        "@angular/platform-browser-dynamic": "^18.2.0",
        "@angular/router": "^18.2.0",
        "@ng-bootstrap/ng-bootstrap": "^17.0.1",
        "@ngneat/until-destroy": "^10.0.0",
        "@popperjs/core": "^2.11.8",
        "@swimlane/ngx-charts": "^21.0.0",
        "bootstrap": "^5.3.2",
        "chart.js": "^4.4.7",
        "chartjs-adapter-date-fns": "^3.0.0",
        "chartjs-plugin-autocolors": "^0.3.1",
        "date-fns": "^4.1.0",
        "rxjs": "~7.8.0",
        "three": "^0.171.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.10"
    },
    "devDependencies": {
