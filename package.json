        "@angular/compiler-cli": "^18.2.0",
        "@angular/localize": "^18.2.0",
        "@openapitools/openapi-generator-cli": "^2.15.3",
        "@types/jasmine": "~5.1.0",
        "jasmine-core": "~5.2.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
